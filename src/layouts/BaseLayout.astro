---
import "~/styles/global.scss";

import nInitial from "~/images/n-initial.svg";

export interface Props {
    title: string;
}

const { title } = Astro.props;
---


<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href={nInitial.src} />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <slot />
  </body>
</html>